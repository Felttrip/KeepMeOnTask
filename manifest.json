{
  "manifest_version": 2,

  "name": "Keep Me On Task",
  "description": "This App will keep you on task",
  "version": "1.0",
  "browser_action": {
    "default_icon": "qrcode.png",
	"default_title": "Keep Me On Task!"
  },
  "background":{
    "persistent":true,
    "scripts":["background.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}
